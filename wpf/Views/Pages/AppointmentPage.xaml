                            <TextBlock HorizontalAlignment="Right" TextAlignment="Right" Text="{Binding IdAppointment}"></TextBlock>
                        </DockPanel>
                    </Border>
                    <Border BorderBrush="#EDEDED" BorderThickness=" 0 0 0 1" MinWidth="300" Margin="0 10 0 0">
